#include <iostream>
#include <string>

using namespace std;

int readnumber()
{
	int number;
	cout << "please enter number ?" << endl;
	cin >> number;

	return number;
}

void printrange(int n)
{
	for (int i = 1; i <= n; i++)
	{
		cout << i << endl;
	}
}

int main()
{
	
	printrange(readnumber());

	return 0;
}
