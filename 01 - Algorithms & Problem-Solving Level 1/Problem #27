#include <iostream>
#include <string>

using namespace std;

int readnumber()
{
	int number;
	cout << "please enter number ?" << endl;
	cin >> number;

	return number;
}

void printNto1(int n)
{
	for (int i = n; i >= 1; i--)
	{
		cout << i << endl;
	}
}

int main()
{
	
	printNto1(readnumber());

	return 0;
}
