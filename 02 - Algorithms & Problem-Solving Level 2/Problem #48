#include <iostream>
#include <cstdlib>
#include <string>
#include <cmath>
using namespace std;

float readnumber(string massege)
{
	float number = 0;
	cout << massege << endl;
	cin >> number;

	return number;
}

int myfloor(float number)
{
	if (number >= 0)
	{
		return int(number);
	}
	else
	{
		return int(number) - 1;
	}

}

int main()
{
	float number = readnumber(" ");

	cout << "my floor result : " << myfloor(number) << endl;

	cout << "C++ floor result : " << floor(number) << endl;

	return 0;
}
