#include <iostream>
#include <string>
#include <cstdlib>

using namespace std;

int randomnumber(int from, int to)
{
	int randnumber = rand() % (to - from + 1) + from;

	return randnumber;

}

int main()
{
	srand((unsigned)time(NULL));


	for (int i = 1; i <= 3; i++)
	{
		cout << randomnumber(1, 10) << endl;
	}
	

	return 0;
}
